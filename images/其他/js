function formatCSTDate(n){return n.replace("T"," ")}function autoCarousel(){var t,n,u,i;try{t=$(".banner .move li"),n=0;function r(){var u=$(t)[n],r;$(u).addClass("none"),n+1>=t.length?n=0:n+=1,r=$(t)[n],GETAO.IsLAOWANG($(r).find("img").attr("src"))&&$(r).find("img").attr("src",$(r).find("img").attr("data-asrc")),$(r).hasClass("none")&&$(r).removeClass("none"),i.removeClass("curr"),i.eq(n).addClass("curr")}u=setInterval(r,6e3),i=$(".banner .switch").children("a"),i.mouseenter(function(){n=$(this).index(),n-=1,r(),clearInterval(u)}).mouseleave(function(){u=setInterval(r,6e3)});function f(){n<0?n=t.length-1:n>=t.length&&(n=0);var r=$(t)[n];GETAO.IsLAOWANG($(r).find("img").attr("src"))&&$(r).find("img").attr("src",$(r).find("img").attr("data-asrc")),$(r).hasClass("none")&&$(r).removeClass("none"),i.removeClass("curr"),i.eq(n).addClass("curr")}$("#prev-bg").click(function(){if(t.length!=1){var i=$(t)[n];$(i).addClass("none"),n=$(".banner .switch .curr").index(),n-=1,f()}}),$("#next-bg").click(function(){if(t.length!=1){var i=$(t)[n];$(i).addClass("none"),n=$(".banner .switch .curr").index(),n+=1,f()}})}catch(e){alert(e.message)}}$(function(){$(".tab_nav .navfirst li").click(function(){$(this).addClass("curr").siblings().removeClass("curr"),$(this).find("i").addClass("sel").parent().siblings().find("i").removeClass("sel"),$(this).find("i").addClass("sel_white").parent().siblings().find("i").removeClass("sel_white");var n=$(this).attr("data-contain");$.each($(this).parent().parent().find(".contain").children(),function(t,i){$(i).attr("data-contain")==n?$(i).show():$(i).hide()})}),$(".contain .item_ul .click_more").click(function(){$(this).parent(".column_lf").parent(".item_main").siblings(".more_con").toggle("fast")}),$(".contain .item_ul .add_btn").click(function(){$(this).siblings(".reply_box").show(),$(this).hide()}),$(".more_con .info_date a").click(function(){$(this).parent(".info_date").parent("li").parent("ul").siblings(".add_reply").find(".reply_box").show(),$(this).parent(".info_date").parent("li").parent("ul").siblings(".add_reply").find(".add_btn").hide()}),$(".city_txt").click(function(){$(this).hasClass("curr")?($(this).removeClass("curr"),$("#city_con").hide("fast")):($(this).addClass("curr"),$("#city_con").show("fast"))}),$(".city_pr").mouseleave(function(){$(this).find(".city_txt").removeClass("curr"),$("#city_con").hide()}),$(".trade-se").click(function(){$(this).hasClass("curr")?($(this).removeClass("curr"),$("#trade-con").hide()):($(this).addClass("curr"),$("#trade-con").show("fast"))}),$(".my-trade").mouseleave(function(){$(this).find(".trade-se").removeClass("curr"),$("#trade-con").hide()}),$(".list_item").mouseenter(function(){$(this).addClass("list_item_hover")}),$(".list_item").mouseleave(function(){$(this).removeClass("list_item_hover")}),$("#back_top").hide(),$(window).scroll(function(){$(window).scrollTop()>100?$("#back_top").show():$("#back_top").hide()}),$("#back_top").click(function(){$("html,body").animate({scrollTop:0},500)}),autoCarousel();try{console.log("%c《比印集市》，一个专注于印刷服务的网站。","font-size:30px;")}catch(n){}$("#APP-mouse").mouseenter(function(){$(this).find(".APPpop-up").show()}),$("#APP-mouse").mouseleave(function(){$(this).find(".APPpop-up").hide()}),$("#Tripartite-mouse").mouseenter(function(){$(this).find(".tripop-up").show()}),$("#Tripartite-mouse").mouseleave(function(){$(this).find(".tripop-up").hide()})})