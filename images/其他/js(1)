function signOut(){try{$.ajax({url:"/Master/ZHttpPost_SignOut",type:"POST",async:!1,dataType:"Json",success:function(n){n.ResponseCode=="0"?location.href=location.href:GETAO.ShowLightMsgAdv("tip_box",n.ResponseMessage,GETAO_LMT_ERROR,-60,3,!0)}})}catch(n){GETAO.ShowLightMsgAdv("tip_box","用户退出失败",GETAO_LMT_ERROR,-60,3,!0)}}function InsertCookieCity(){var i=$("#curCity").attr("data-cityfpi"),t=$("#curCity").attr("data-cityname"),r=$("#curCity").attr("data-lng"),u=$("#curCity").attr("data-lat"),f=$("#curCity").attr("data-maplevel"),n;$("#curCity").html(t),GETAO.IsNullOrEmptyOrUndefined(i)&&(i="bj"),GETAO.IsNullOrEmptyOrUndefined(t)&&(t="北京"),GETAO.IsNullOrUndefined(r)&&(r=0),GETAO.IsNullOrUndefined(u)&&(u=0),GETAO.IsNullOrEmptyOrUndefined(f)&&(f=12),n=new City,n.FPI=i,n.Name=t,n.Point.Lng=r,n.Point.Lat=u,n.MLevel=f,window.location.host=="localhost:50520"?GETAO.Cookie.InsertCookie("BYWCURCITY",encodeURI(JSON2.stringify(n)),7,"/",null):GETAO.Cookie.InsertCookie("BYWCURCITY",encodeURI(JSON2.stringify(n)),7,"/",null)}$(function(){if(1)try{var n=GETAO.Cookie.GetCookie("BYWCURCITY");if(n)n=JSON2.parse(unescape(decodeURI(n))),$("#curCity").attr("data-cityfpi",n.FPI),$("#curCity").attr("data-cityname",n.Name),$("#curCity").attr("data-lng",n.Point.Lng),$("#curCity").attr("data-lat",n.Point.Lat),$("#curCity").attr("data-maplevel",n.MLevel),$("#curCity").html(n.Name),$("#topMenu_Sellers").attr("href","/sellers/"+n.FPI+"/"),$("#topMenu_NearBy").attr("href","/nearby/"+n.FPI+"/"),$("#topMenu_Meeting").attr("href","/meeting/DP20164QNHC/user4pc4byw/"+n.FPI+"/");else{try{(new BMap.LocalCity).get(function(n){n&&n.name!="北京市"&&$.ajax({url:"/master/ZHttpPost_GetCityByBaiDuIPLocal",data:{cityName:encodeURI(n.name)},type:"POST",async:!1,dataType:"Json",success:function(n){var i,t;n.ResponseCode=="0"&&(i=JSON2.parse(n.ResponseData),$("#curCity").attr("data-cityfpi",i.SubDomainCode),t=i.Name,t.substring(t.length-1)=="市"&&(t=t.substring(0,t.length-1)),$("#curCity").attr("data-cityname",t),$("#curCity").attr("data-lng",i.Lng),$("#curCity").attr("data-lat",i.Lat),$("#curCity").attr("data-maplevel",i.mLevle))}}),InsertCookieCity()})}catch(t){}GETAO.Cookie.GetCookie("BYWCURCITY")||InsertCookieCity()}}catch(t){}});$(document).on("click",".citiessel a",function(){var n,i,r,u,f,t;try{if($(this).attr("data-enable")=="1")return;n=$(this).attr("data-cityfpi"),GETAO.IsNullOrEmptyOrUndefined(n)&&(n="bj"),i=$(this).attr("data-cityname"),$.ajax({url:"/Master/ZHttpPost_GetCityLatAndLng",data:{cityDomain:n},type:"POST",async:!1,dataType:"Json",success:function(n){if(n.ResponseCode=="0"){var t=JSON2.parse(n.ResponseData);r=t.Lng,u=t.Lat,f=t.Mlevel}}}),GETAO.IsNullOrEmptyOrUndefined(i)&&(i="北京"),GETAO.IsNullOrUndefined(r)&&(r=116.404),GETAO.IsNullOrUndefined(u)&&(u=39.915),GETAO.IsNullOrEmptyOrUndefined(f)&&(f=12),$("#curCity").attr("data-cityfpi",n),$("#curCity").attr("data-cityname",i),$("#curCity").attr("data-lng",r),$("#curCity").attr("data-lat",u),$("#curCity").attr("data-maplevel",f),$("#curCity").html(i),t=new City,t.FPI=n,t.Name=i,t.Point.Lng=r,t.Point.Lat=u,t.MLevel=f,window.location.host=="localhost:50520"?GETAO.Cookie.InsertCookie("BYWCURCITY",encodeURI(JSON2.stringify(t)),7,"/",null):GETAO.Cookie.InsertCookie("BYWCURCITY",encodeURI(JSON2.stringify(t)),7,"/",null),!0&&$("#topMenu_All li a").each(function(t,i){if($(i).hasClass("curr")){$(i).attr("data-pageflag")=="sellers"?window.location=$(i).attr("data-pagehref")+"/"+n+"/":$(i).attr("data-pageflag")=="nearby"?window.location=$(i).attr("data-pagehref")+"/"+n+"/":$(i).attr("data-pageflag")=="meeting"?window.location.href=$(i).attr("data-pagehref")+"/"+n+"/":window.location.reload();return}}),window.location.href.indexOf("/cities")!=-1&&(window.location.href="/sellers/"+n+"/")}catch(e){!0}})