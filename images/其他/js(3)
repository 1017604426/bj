function OpenWin(){window.open("/xtools/IMMessage","NewWindow","width=770,height=600,top=300,left=300,toolbar=no,menubar=no,scrollbars=no,resizable=no,location=no,status=no")}function OpenIMWindow(){try{var n=window.open("/xtools/IMMessage","NewWindow","width=770,height=600,top=300,left=300,toolbar=no,menubar=no,scrollbars=no,resizable=no,location=no,status=no");n!=null&&n.focus()}catch(t){alert("当前浏览器不支持在线客服功能。")}}